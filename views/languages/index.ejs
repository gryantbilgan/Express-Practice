<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Languages Learned</h1>
    <ul>
        <% languages.forEach(function(l) { %>
            <li>
                <%= l.language %>
                -
                <%= l.done ? 'done' : 'not done' %>
            </li>
        <% }); %>
    </ul>
</body>
</html>